AAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,OALA,EAKA,KALA,CAKA,YAAA,CACA,CANA;AAOA;AAnFA;AAlCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <el-row :gutter=\"10\" class=\"mb8\">\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n            type=\"primary\"\r\n            plain\r\n            icon=\"el-icon-plus\"\r\n            size=\"mini\"\r\n            @click=\"handleAdd\"\r\n        >新增\r\n        </el-button>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-table v-loading=\"loading\" :data=\"clothesList\">\r\n      <el-table-column label=\"编号\" align=\"center\" prop=\"id\"/>\r\n      <el-table-column label=\"名称\" align=\"center\" prop=\"name\"/>\r\n      <el-table-column label=\"金币价格\" align=\"center\" prop=\"price\"/>\r\n      <!-- <el-table-column label=\"购买此衣服的用户\" align=\"center\" prop=\"userIds\"/> -->\r\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n              size=\"mini\"\r\n              type=\"text\"\r\n              icon=\"el-icon-edit\"\r\n              @click=\"handleUpdate(scope.row)\"\r\n          >修改\r\n          </el-button>\r\n          <el-button\r\n              size=\"mini\"\r\n              type=\"text\"\r\n              icon=\"el-icon-delete\"\r\n              @click=\"handleDelete(scope.row)\"\r\n          >删除\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-pagination\r\n        background\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :current-page=\"queryParams.pageNum\"\r\n        :page-size=\"queryParams.pageSize\"\r\n        :page-sizes=\"[10,20,50]\"\r\n        :total=\"total\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handlePageChange\"\r\n    />\r\n\r\n\r\n    <!-- 添加或修改橱窗管理对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"50%\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"90px\">\r\n        <el-form-item label=\"名称\" prop=\"name\">\r\n          <el-input v-model=\"form.name\" placeholder=\"请输入名称\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"金币价格\" prop=\"price\">\r\n          <el-input v-model=\"form.price\" placeholder=\"请输入金币价格\"/>\r\n        </el-form-item>\r\n        <!-- <el-form-item label=\"购买此衣服的用户\" prop=\"userIds\">\r\n          <el-input v-model=\"form.userIds\" placeholder=\"请输入购买此衣服的用户\"/>\r\n        </el-form-item> -->\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {listClothes, getClothes, delClothes, addClothes, updateClothes} from \"@/api/project/clothes\";\r\n\r\nexport default {\r\n  name: \"Clothes\",\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      // 总条数\r\n      total: 0,\r\n      // 橱窗管理表格数据\r\n      clothesList: [],\r\n      // 弹出层标题\r\n      title: \"\",\r\n      // 是否显示弹出层\r\n      open: false,\r\n      // 查询参数\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: null,\r\n        price: null,\r\n        userIds: null\r\n      },\r\n      // 表单参数\r\n      form: {},\r\n      // 表单校验\r\n      rules: {\r\n        id: [\r\n          {required: true, message: \"编号不能为空\", trigger: \"blur\"}\r\n        ],\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n    handlePageChange(currentPage) {\r\n      this.queryParams.pageNum = currentPage\r\n      this.getList()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      this.queryParams.pageSize = pageSize\r\n      this.getList()\r\n    },\r\n    /** 查询橱窗管理列表 */\r\n    getList() {\r\n      this.loading = true;\r\n      listClothes(this.queryParams).then(response => {\r\n        let list, start, end\r\n        start = (this.queryParams.pageNum - 1) * this.queryParams.pageSize\r\n        end = start + this.queryParams.pageSize\r\n        list = response.data.filter((item, index) => {\r\n          return index >= start && index < end\r\n        })\r\n        this.clothesList = list;\r\n        this.total = response.data.length;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.open = false;\r\n      this.reset();\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        id: null,\r\n        name: null,\r\n        price: null,\r\n        userIds: null\r\n      };\r\n      // this.resetForm(\"form\");\r\n    },\r\n    /** 新增按钮操作 */\r\n    handleAdd() {\r\n      this.reset();\r\n      this.open = true;\r\n      this.title = \"添加橱窗管理\";\r\n    },\r\n    /** 修改按钮操作 */\r\n    handleUpdate(row) {\r\n      this.reset();\r\n      getClothes(row).then(response => {\r\n        this.form = response.data;\r\n        this.open = true;\r\n        this.title = \"修改橱窗管理\";\r\n      });\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate(valid => {\r\n        if (valid) {\r\n          if (this.form.id != null) {\r\n            updateClothes(this.form).then(response => {\r\n              this.$message.success(\"修改成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          } else {\r\n            addClothes(this.form).then(response => {\r\n              this.$message.success(\"新增成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    /** 删除按钮操作 */\r\n    handleDelete(row) {\r\n      this.$confirm('是否确认删除？').then(function () {\r\n        return delClothes(row);\r\n      }).then(() => {\r\n        this.getList();\r\n        this.$message.success(\"删除成功\");\r\n      }).catch(() => {\r\n      });\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n"],"sourceRoot":"src/views/clothes"}]}
AHA,OAAA,CAAA,CAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,WAAA;AAEA,SAJA,MAIA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,UAAA,EAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA,EADA,CAEA;;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AAEA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,aAAA;AAEA;AACA,OAjBA;AAmBA;AA3CA;AAdA,CAAA","sourcesContent":["<template>\r\n  <div class=\"login-main\">\r\n    <!-- 登录主题 -->\r\n    <div class=\"input-main\">\r\n      <div class=\"login-title\">宠物后台管理系统登录</div>\r\n      <div>\r\n        <div class=\"label\">账号</div>\r\n        <div>\r\n          <el-input\r\n              v-model=\"loginInfo.number\"\r\n              placeholder=\"请输入账号\"\r\n              clearable\r\n          ></el-input>\r\n        </div>\r\n      </div>\r\n      <div>\r\n        <div class=\"label\">密码</div>\r\n        <div>\r\n          <el-input\r\n              v-model=\"loginInfo.pws\"\r\n              type=\"password\"\r\n              placeholder=\"请输入密码\"\r\n              clearable\r\n          ></el-input>\r\n        </div>\r\n      </div>\r\n      <div>\r\n        <div class=\"label\">验证码</div>\r\n        <div>\r\n          <el-row>\r\n            <el-col :span=\"17\">\r\n              <el-input\r\n                  v-model=\"loginInfo.verificationCode\"\r\n                  placeholder=\"请输入验证码\"\r\n                  clearable\r\n              ></el-input>\r\n            </el-col>\r\n            <el-col :span=\"5\">\r\n              <VerifyCode/>\r\n            </el-col>\r\n          </el-row>\r\n        </div>\r\n      </div>\r\n      <div style=\"display: flex;margin: 10px auto\">\r\n        <!--        <el-radio v-model=\"loginInfo.userPermissions\" label=\"普通用户\">普通用户</el-radio>-->\r\n        <el-radio v-model=\"loginInfo.userPermissions\" label=\"管理员\">管理员</el-radio>\r\n      </div>\r\n      <div>\r\n        <el-button type=\"primary\" style=\"width: 100%\" @click=\"login\">\r\n          登录\r\n        </el-button>\r\n        <!--        <el-button  style=\"width: 100%;margin-top: 10px;margin-left:0px\" @click=\"toregister\">-->\r\n        <!--          注册-->\r\n        <!--        </el-button>-->\r\n\r\n\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport VerifyCode from '../../components/VerificationCode'\r\nimport {login} from \"@/api/project/user\";\r\n\r\nexport default {\r\n  components: {\r\n    VerifyCode,\r\n  },\r\n  data() {\r\n    return {\r\n      loginInfo: {\r\n        number: '',\r\n        pws: '',\r\n        verificationCode: '',\r\n        userPermissions: \"管理员\"\r\n      },\r\n    }\r\n  },\r\n  methods: {\r\n    // toregister(){\r\n    //    this.$router.push('/register')\r\n    // },\r\n    login() {\r\n      if (!this.loginInfo.number) {\r\n        this.$message.error('请输入账号')\r\n        return\r\n      }\r\n      if (!this.loginInfo.pws) {\r\n        this.$message.error('请输入密码')\r\n        return\r\n      }\r\n      if (!this.loginInfo.verificationCode) {\r\n        this.$message.error('请输入验证码')\r\n        return\r\n      }\r\n\r\n      if (this.loginInfo.verificationCode !== localStorage.getItem(\"identifyCode\")) {\r\n        this.$message.error('验证码错误')\r\n        return\r\n      }\r\n\r\n\r\n      login({\r\n        userAccount: this.loginInfo.number,\r\n        userPassword: this.loginInfo.pws,\r\n        userPermissions: this.loginInfo.userPermissions,\r\n      }).then(res => {\r\n        if (res.data.length == 0) {\r\n          // 弹出通知框提示登录失败信息\r\n          this.$message.error(\"账号或密码输入错误\");\r\n\r\n        } else {\r\n          localStorage.setItem('userInfo', JSON.stringify(res.data[0]))\r\n          // 弹出通知框提示登录成功信息\r\n          this.$message.success(\"登陆成功\");\r\n\r\n          this.$router.push('/gameRecord')\r\n\r\n        }\r\n      })\r\n\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.login-main {\r\n\r\n  position: absolute;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-image: url('../../assets/loginbg2.png');\r\n  background-size: 100% 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n\r\n  .input-main {\r\n    opacity: 0.9;\r\n    width: 500px;\r\n    height: 500px;\r\n    background-color: white;\r\n    border-radius: 5px;\r\n    padding: 40px;\r\n    box-sizing: border-box;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    gap: 20px;\r\n  }\r\n}\r\n\r\n.login-title {\r\n  color: #409eff;\r\n  font-size: 25px;\r\n  font-weight: 700;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.label {\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/login"}]}
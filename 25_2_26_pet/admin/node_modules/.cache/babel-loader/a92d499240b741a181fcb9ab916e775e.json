;AAKA,IAAA,gBALA,4BAKA,QALA,EAKA;AACA,WAAA,WAAA,CAAA,QAAA,GAAA,QAAA;AACA,WAAA,OAAA;AACA,KARA;;AASA;AACA,IAAA,OAVA,qBAUA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,MAAA,UAAA,CAAA,KAAA,WAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,IAAA,EAAA,KAAA,EAAA,GAAA;AACA,QAAA,KAAA,GAAA,CAAA,KAAA,CAAA,WAAA,CAAA,OAAA,GAAA,CAAA,IAAA,KAAA,CAAA,WAAA,CAAA,QAAA;AACA,QAAA,GAAA,GAAA,KAAA,GAAA,KAAA,CAAA,WAAA,CAAA,QAAA;AACA,QAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,iBAAA,KAAA,IAAA,KAAA,IAAA,KAAA,GAAA,GAAA;AACA,SAFA,CAAA;AAGA,QAAA,KAAA,CAAA,UAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAVA;AAWA,KAvBA;AAwBA;AACA,IAAA,MAzBA,oBAyBA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,KAAA;AACA,KA5BA;AA6BA;AACA,IAAA,KA9BA,mBA8BA;AACA,WAAA,IAAA,GAAA;AACA,QAAA,EAAA,EAAA,IADA;AAEA,QAAA,MAAA,EAAA,IAFA;AAGA,QAAA,KAAA,EAAA,IAHA;AAIA,QAAA,UAAA,EAAA,IAJA;AAKA,QAAA,WAAA,EAAA,IALA;AAMA,QAAA,IAAA,EAAA;AANA,OAAA,CADA,CASA;AACA,KAxCA;;AAyCA;AACA,IAAA,SA1CA,uBA0CA;AACA,WAAA,KAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,KAAA,GAAA,QAAA;AACA,KA9CA;;AA+CA;AACA,IAAA,YAhDA,wBAgDA,GAhDA,EAgDA;AAAA;;AACA,WAAA,KAAA;AACA,MAAA,SAAA,CAAA,GAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA;AACA,OAJA;AAKA,KAvDA;;AAwDA;AACA,IAAA,UAzDA,wBAyDA;AAAA;;AACA,WAAA,KAAA,CAAA,MAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,MAAA,CAAA,IAAA,CAAA,EAAA,IAAA,IAAA,EAAA;AACA,YAAA,YAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,IAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,OAAA;AACA,aAJA;AAKA,WANA,MAMA;AACA,YAAA,SAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,IAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,OAAA;AACA,aAJA;AAKA;AACA;AACA,OAhBA;AAiBA,KA3EA;;AA4EA;AACA,IAAA,YA7EA,wBA6EA,GA7EA,EA6EA;AAAA;;AACA,WAAA,QAAA,CAAA,SAAA,EAAA,IAAA,CAAA,YAAA;AACA,eAAA,SAAA,CAAA,GAAA,CAAA;AACA,OAFA,EAEA,IAFA,CAEA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,OALA,EAKA,KALA,CAKA,YAAA,CACA,CANA;AAOA;AArFA;AApCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <!-- <el-row :gutter=\"10\" class=\"mb8\">\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n            type=\"primary\"\r\n            plain\r\n            icon=\"el-icon-plus\"\r\n            size=\"mini\"\r\n            @click=\"handleAdd\"\r\n        >新增\r\n        </el-button>\r\n      </el-col>\r\n    </el-row> -->\r\n\r\n    <el-table v-loading=\"loading\" :data=\"recordList\">\r\n      <el-table-column label=\"编号\" align=\"center\" prop=\"id\"/>\r\n      <el-table-column label=\"用户ID\" align=\"center\" prop=\"userId\"/>\r\n      <el-table-column label=\"宠物ID\" align=\"center\" prop=\"petId\"/>\r\n      <el-table-column label=\"获得的心情值\" align=\"center\" prop=\"moodGained\"/>\r\n      <el-table-column label=\"类型\" align=\"center\" prop=\"type\"/>\r\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n              size=\"mini\"\r\n              type=\"text\"\r\n              icon=\"el-icon-edit\"\r\n              @click=\"handleUpdate(scope.row)\"\r\n          >修改\r\n          </el-button>\r\n          <el-button\r\n              size=\"mini\"\r\n              type=\"text\"\r\n              icon=\"el-icon-delete\"\r\n              @click=\"handleDelete(scope.row)\"\r\n          >删除\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-pagination\r\n        background\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :current-page=\"queryParams.pageNum\"\r\n        :page-size=\"queryParams.pageSize\"\r\n        :page-sizes=\"[10,20,50]\"\r\n        :total=\"total\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handlePageChange\"\r\n    />\r\n\r\n\r\n    <!-- 添加或修改【请填写功能名称】对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"50%\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"90px\">\r\n        <el-form-item label=\"用户ID\" prop=\"userId\">\r\n          <el-input v-model=\"form.userId\" placeholder=\"请输入用户ID\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"宠物ID\" prop=\"petId\">\r\n          <el-input v-model=\"form.petId\" placeholder=\"请输入宠物ID\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"获得的心情值\" prop=\"moodGained\">\r\n          <el-input v-model=\"form.moodGained\" placeholder=\"请输入获得的心情值\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"记录时间\" prop=\"createdTime\">\r\n          <el-input v-model=\"form.createdTime\" placeholder=\"请输入记录时间\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"类型\" prop=\"type\">\r\n          <el-select v-model=\"form.type\" placeholder=\"请选择类型\">\r\n            <el-option label=\"2048合成\" value=\"2048合成\"/>\r\n            <el-option label=\"猜成语\" value=\"猜成语\"/>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {listRecord, getRecord, delRecord, addRecord, updateRecord} from \"@/api/project/gameRecord\";\r\n\r\nexport default {\r\n  name: \"Record\",\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      // 总条数\r\n      total: 0,\r\n      // 【请填写功能名称】表格数据\r\n      recordList: [],\r\n      // 弹出层标题\r\n      title: \"\",\r\n      // 是否显示弹出层\r\n      open: false,\r\n      // 查询参数\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        userId: null,\r\n        petId: null,\r\n        moodGained: null,\r\n        createdTime: null,\r\n        type: null\r\n      },\r\n      // 表单参数\r\n      form: {},\r\n      // 表单校验\r\n      rules: {\r\n        id: [\r\n          {required: true, message: \"编号不能为空\", trigger: \"blur\"}\r\n        ],\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n    handlePageChange(currentPage) {\r\n      this.queryParams.pageNum = currentPage\r\n      this.getList()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      this.queryParams.pageSize = pageSize\r\n      this.getList()\r\n    },\r\n    /** 查询【请填写功能名称】列表 */\r\n    getList() {\r\n      this.loading = true;\r\n      listRecord(this.queryParams).then(response => {\r\n        let list, start, end\r\n        start = (this.queryParams.pageNum - 1) * this.queryParams.pageSize\r\n        end = start + this.queryParams.pageSize\r\n        list = response.data.filter((item, index) => {\r\n          return index >= start && index < end\r\n        })\r\n        this.recordList = list;\r\n        this.total = response.data.length;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.open = false;\r\n      this.reset();\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        id: null,\r\n        userId: null,\r\n        petId: null,\r\n        moodGained: null,\r\n        createdTime: null,\r\n        type: null\r\n      };\r\n      // this.resetForm(\"form\");\r\n    },\r\n    /** 新增按钮操作 */\r\n    handleAdd() {\r\n      this.reset();\r\n      this.open = true;\r\n      this.title = \"添加游戏记录\";\r\n    },\r\n    /** 修改按钮操作 */\r\n    handleUpdate(row) {\r\n      this.reset();\r\n      getRecord(row).then(response => {\r\n        this.form = response.data;\r\n        this.open = true;\r\n        this.title = \"修改游戏记录\";\r\n      });\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate(valid => {\r\n        if (valid) {\r\n          if (this.form.id != null) {\r\n            updateRecord(this.form).then(response => {\r\n              this.$message.success(\"修改成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          } else {\r\n            addRecord(this.form).then(response => {\r\n              this.$message.success(\"新增成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    /** 删除按钮操作 */\r\n    handleDelete(row) {\r\n      this.$confirm('是否确认删除？').then(function () {\r\n        return delRecord(row);\r\n      }).then(() => {\r\n        this.getList();\r\n        this.$message.success(\"删除成功\");\r\n      }).catch(() => {\r\n      });\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n"],"sourceRoot":"src/views/gameRecord"}]}
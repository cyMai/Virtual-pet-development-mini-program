AUA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,MAAA,SAAA,CAAA,KAAA,WAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,IAAA,EAAA,KAAA,EAAA,GAAA;AACA,QAAA,KAAA,GAAA,CAAA,KAAA,CAAA,WAAA,CAAA,OAAA,GAAA,CAAA,IAAA,KAAA,CAAA,WAAA,CAAA,QAAA;AACA,QAAA,GAAA,GAAA,KAAA,GAAA,KAAA,CAAA,WAAA,CAAA,QAAA;AACA,QAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,iBAAA,KAAA,IAAA,KAAA,IAAA,KAAA,GAAA,GAAA;AACA,SAFA,CAAA;AAGA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAVA;AAWA,KAvBA;AAwBA;AACA,IAAA,MAzBA,oBAyBA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,KAAA;AACA,KA5BA;AA6BA;AACA,IAAA,KA9BA,mBA8BA;AACA,WAAA,IAAA,GAAA;AACA,QAAA,EAAA,EAAA,IADA;AAEA,QAAA,GAAA,EAAA;AAFA,OAAA,CADA,CAKA;AACA,KApCA;;AAqCA;AACA,IAAA,SAtCA,uBAsCA;AACA,WAAA,KAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,KAAA,GAAA,MAAA;AACA,KA1CA;;AA2CA;AACA,IAAA,YA5CA,wBA4CA,GA5CA,EA4CA;AAAA;;AACA,WAAA,KAAA;AACA,MAAA,QAAA,CAAA,GAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,MAAA;AACA,OAJA;AAKA,KAnDA;;AAoDA;AACA,IAAA,UArDA,wBAqDA;AAAA;;AACA,WAAA,KAAA,CAAA,MAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,MAAA,CAAA,IAAA,CAAA,EAAA,IAAA,IAAA,EAAA;AACA,YAAA,WAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,IAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,OAAA;AACA,aAJA;AAKA,WANA,MAMA;AACA,YAAA,QAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,IAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,OAAA;AACA,aAJA;AAKA;AACA;AACA,OAhBA;AAiBA,KAvEA;;AAwEA;AACA,IAAA,YAzEA,wBAyEA,GAzEA,EAyEA;AAAA;;AACA,WAAA,QAAA,CAAA,SAAA,EAAA,IAAA,CAAA,YAAA;AACA,eAAA,QAAA,CAAA,GAAA,CAAA;AACA,OAFA,EAEA,IAFA,CAEA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,OALA,EAKA,KALA,CAKA,YAAA,CACA,CANA;AAOA;AAjFA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <el-row :gutter=\"10\" class=\"mb8\">\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n            type=\"primary\"\r\n            plain\r\n            icon=\"el-icon-plus\"\r\n            size=\"mini\"\r\n            @click=\"handleAdd\"\r\n        >新增\r\n        </el-button>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-table v-loading=\"loading\" :data=\"pagesList\">\r\n      <el-table-column label=\"编号\" align=\"center\" prop=\"id\"/>\r\n      <el-table-column label=\"图片\" align=\"center\" prop=\"img\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <div v-if=\"!scope.row.img\">暂无图像</div>\r\n          <div v-else>\r\n            <el-image\r\n                style=\"width: 50px; height: 50px\"\r\n                :src=\"scope.row.img\"\r\n                :preview-src-list=\"[scope.row.img]\">\r\n            </el-image>\r\n          </div>\r\n\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n              size=\"mini\"\r\n              type=\"text\"\r\n              icon=\"el-icon-edit\"\r\n              @click=\"handleUpdate(scope.row)\"\r\n          >修改\r\n          </el-button>\r\n          <el-button\r\n              size=\"mini\"\r\n              type=\"text\"\r\n              icon=\"el-icon-delete\"\r\n              @click=\"handleDelete(scope.row)\"\r\n          >删除\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-pagination\r\n        background\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :current-page=\"queryParams.pageNum\"\r\n        :page-size=\"queryParams.pageSize\"\r\n        :page-sizes=\"[10,20,50]\"\r\n        :total=\"total\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handlePageChange\"\r\n    />\r\n\r\n\r\n    <!-- 添加或修改【请填写功能名称】对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"50%\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"90px\">\r\n        <el-form-item label=\"图片\" prop=\"img\">\r\n          <image-upload v-model=\"form.img\"></image-upload>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {listPages, getPages, delPages, addPages, updatePages} from \"@/api/project/pages\";\r\n\r\nexport default {\r\n  name: \"Pages\",\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      // 总条数\r\n      total: 0,\r\n      // 【请填写功能名称】表格数据\r\n      pagesList: [],\r\n      // 弹出层标题\r\n      title: \"\",\r\n      // 是否显示弹出层\r\n      open: false,\r\n      // 查询参数\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        img: null\r\n      },\r\n      // 表单参数\r\n      form: {},\r\n      // 表单校验\r\n      rules: {\r\n        id: [\r\n          {required: true, message: \"编号不能为空\", trigger: \"blur\"}\r\n        ],\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n    handlePageChange(currentPage) {\r\n      this.queryParams.pageNum = currentPage\r\n      this.getList()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      this.queryParams.pageSize = pageSize\r\n      this.getList()\r\n    },\r\n    /** 查询【请填写功能名称】列表 */\r\n    getList() {\r\n      this.loading = true;\r\n      listPages(this.queryParams).then(response => {\r\n        let list, start, end\r\n        start = (this.queryParams.pageNum - 1) * this.queryParams.pageSize\r\n        end = start + this.queryParams.pageSize\r\n        list = response.data.filter((item, index) => {\r\n          return index >= start && index < end\r\n        })\r\n        this.pagesList = list;\r\n        this.total = response.data.length;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.open = false;\r\n      this.reset();\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        id: null,\r\n        img: null\r\n      };\r\n      // this.resetForm(\"form\");\r\n    },\r\n    /** 新增按钮操作 */\r\n    handleAdd() {\r\n      this.reset();\r\n      this.open = true;\r\n      this.title = \"添加知识\";\r\n    },\r\n    /** 修改按钮操作 */\r\n    handleUpdate(row) {\r\n      this.reset();\r\n      getPages(row).then(response => {\r\n        this.form = response.data;\r\n        this.open = true;\r\n        this.title = \"修改知识\";\r\n      });\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate(valid => {\r\n        if (valid) {\r\n          if (this.form.id != null) {\r\n            updatePages(this.form).then(response => {\r\n              this.$message.success(\"修改成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          } else {\r\n            addPages(this.form).then(response => {\r\n              this.$message.success(\"新增成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    /** 删除按钮操作 */\r\n    handleDelete(row) {\r\n      this.$confirm('是否确认删除？').then(function () {\r\n        return delPages(row);\r\n      }).then(() => {\r\n        this.getList();\r\n        this.$message.success(\"删除成功\");\r\n      }).catch(() => {\r\n      });\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n"],"sourceRoot":"src/views/pages"}]}